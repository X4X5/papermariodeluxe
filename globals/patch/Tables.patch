	SLL t1, t1, 0003 %multiply by 8
	ADDU t0, t0, t1
	%
	LW a0, 0020 (s3) %xPos
	LH a1, 0034 (s3) %yPos
	LH a2, 0036 (s3)
	LB a3, 0002 (t0) %first number to draw
	LB v0, 0003 (t0) %load partner max hp
	ADDIU a1, a1, 0028
	ADDIU a2, a2, 001C
	JAL 800E7E34
	SW v0, 0010 (sp)
	%
	LW ra, 0018 (sp)
	JR RA
	ADDIU sp, sp, 20
}

