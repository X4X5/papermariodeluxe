#new:Function $DrawPartnerIcon {
	PUSH s0, s1, ra
	LIO a2, 8008F000 %partner icon table
	LABU a1, 8010F2A2
	ORI a0, r0, 0009 %bow partner ID
	BEQ a0, a1, .isBow
	NOP
	BEQ a1, r0, .noSubtract
	NOP
	ADDIU a1, a1, FFFF
	.noSubtract %if no partner, draws goombario.
	SLL a1, a1, 0003 %multiply by 8
	ADDU a0, a2, a1
	LW s1, 0000 (a0) %load partner icon script
	DADDU a0, s1, r0
	JAL ~Func:create_hud_element %alternate name: create_icon
	NOP
	DADDU s0, v0, r0 %iconIndex to s0
	DADDU a0, s0, r0
	JAL ~Func:set_hud_element_flags %alternate name: set_icon_flags
	ORI a1, r0, 0080
	JAL ~Func:clear_hud_element_flags %alternate name: clear_icon_flags
	DADDU a0, s0, r0
	DADDU a0, s0, r0
	LH a1, 0034 (s3)
	ADDIU a1, a1, 000E
	LH a2, 0036 (s3)
	JAL ~Func:set_hud_element_render_pos %alternate name: set_icon_render_pos
	ADDIU a2, a2, 0023
	DADDU a1, s1, r0
	JAL ~Func:set_hud_element_script %alternate name: set_menu_icon_script
	DADDU a0, s0, r0
	LIO a1, 3F000000
	JAL 80144C20 %scale icon to 0.6
	DADDU a0, s0, r0
	JAL ~Func:draw_hud_element_1 %alternate name: draw_icon_1
	DADDU a0, s0, r0
	JAL ~Func:free_hud_element
	DADDU a0, s0, r0
	POP s0, s1, ra
	JR RA
	NOP
	.isBow
	BEQ r0, r0, .noSubtract
	ORI a1, r0, 0004 %bow's partner ID doesn't match the icon script table index
}

