#new:Function $DrawMarioCurAndMaxHP {
	ADDIU sp, sp, -20
	SW ra, 0018 (sp)
	%
	LW a0, 0020 (s3) 
	LH a1, 0034 (s3) %xPos
	LH a2, 0036 (s3) %yPos
	LB a3, 003D (s3) %first number to draw
	LUI v0, 8011
	LB v0, F293 (v0) %load mario's max hp (second number to draw)
	ADDIU a1, a1, 0028
	ADDIU a2, a2, 0008
	JAL 800E7E34
	SW v0, 0010 (sp)
	%
	LW ra, 0018 (sp)
	JR RA
	ADDIU sp, sp, 20
}

#new:Function $DrawCurrentPartnerCurAndMaxHP {
	ADDIU sp, sp, -20
	SW ra, 0018 (sp)
	LIO t0, 8010F2A4 %start of partner structs
