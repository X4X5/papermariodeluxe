@Hook:Global 800E86B0 {
	%%%mario hp box
	LH a0, 0034 (s3) %xPos
	LH a1, 0036 (s3) %yPos
	ADDIU a0, a0, 0000
	ADDIU a1, a1, 0000
	ORI a2, r0, 0060 %width
	ORI a3, r0, 001C %height
	ORI t0, r0, 001A %custom style
	JAL $DrawMenuBox
	ORI t1, r0, 00FF %full opacity

	%%%partner hp box
	LH a0, 0034 (s3) %xPos
	LH a1, 0036 (s3) %yPos
	ADDIU a0, a0, 0000
	ADDIU a1, a1, 0015
	ORI a2, r0, 0060 %width
	ORI a3, r0, 001C %height
	ORI t0, r0, 001A %custom style
	JAL $DrawMenuBox
	ORI t1, r0, 00FF %full opacity

	%%%fp box
	LH a0, 0034 (s3) %xPos
	LH a1, 0036 (s3) %yPos
	ADDIU a0, a0, 005C
	ADDIU a1, a1, 0000
	ORI a2, r0, 0058 %width
	ORI a3, r0, 001C %height
	ORI t0, r0, 0017 %style 0x17
	JAL $DrawMenuBox
	ORI t1, r0, 00FF %full opacity

	%%%coins/exp box
	LH a0, 0034 (s3) %xPos
	LH a1, 0036 (s3) %yPos
	ADDIU a0, a0, 00B2
	ADDIU a1, a1, 0000
	ORI a2, r0, 0074 %width
	ORI a3, r0, 001C %height
	ORI t0, r0, 0019 %style 0
	JAL $DrawMenuBox
	ORI t1, r0, 00FF %full opacity

	%%%star power box
	LABU a0, 8010F51E
	BEQ a0, r0, .noStarPower
	NOP
	LH a0, 0034 (s3) %xPos
	LH a1, 0036 (s3) %yPos
	ADDIU a0, a0, 005C
	ADDIU a1, a1, 0015
	ORI a2, r0, 00B0 %width
	ORI a3, r0, 001C %height
	ORI t0, r0, 001B %style 0x1B
	JAL $DrawMenuBox
	ORI t1, r0, 00FF %full opacity
	.noStarPower
	J 800E8714
	NOP
%0x34 from s3 is X pos of c-up menu
